        app:layout_constraintTop_toTopOf="@+id/pettvimedrugog" />

    <TextView
        android:id="@+id/pettvbodovitreceg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
